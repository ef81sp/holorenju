{
  "name": "holorenju",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "lefthook install",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:browser": "vitest run --project browser",
    "test:browser:headless": "vitest run --project browser --browser.headless",
    "lint": "oxlint && eslint .",
    "lint:fix": "oxlint --fix && eslint --fix .",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "type-check": "vue-tsc -b",
    "check-fix": "pnpm run type-check && pnpm run format && pnpm run lint:fix",
    "check": "pnpm run type-check && pnpm run format:check && pnpm run lint",
    "bench": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/bench-ai.ts",
    "analyze:games": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/analyze-games.ts",
    "browse:games": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/browse-games.ts",
    "analyze:bench": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/analyze-bench.ts",
    "analyze:weakness": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/analyze-weakness.ts",
    "ab:bench": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/ab-bench.ts",
    "tune:params": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/tune-params.ts",
    "analyze:position": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/analyze-position.ts",
    "analyze:deep": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/analyze-bench-deep.ts",
    "debug:vcf": "node --experimental-strip-types --disable-warning=ExperimentalWarning --import ./scripts/register-loader.mjs scripts/debug-vcf-false-positive.ts"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "konva": "^10.0.12",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-konva": "^3.2.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^4.0.17",
    "@vitest/browser-playwright": "^4.0.17",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "^1.36.0",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "lefthook": "^2.1.0",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "playwright": "^1.57.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-pwa": "^1.2.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^4.0.17",
    "vitest-browser-vue": "^2.0.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.4",
    "workbox-window": "^7.4.0"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    },
    "onlyBuiltDependencies": [
      "lefthook"
    ]
  }
}
